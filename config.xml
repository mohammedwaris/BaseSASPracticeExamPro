<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.jlojic.basesaspracticeexampro"
        versionCode = "104" 
        version     = "1.0.4" >

    <!-- versionCode is optional and Android only -->

    <name>Base SAS Practice Exam Pro</name>

    <description>
        An app for people who are preparing for Base SAS certification.
    </description>

    <author href="http://www.jlojic.com" email="jlojic.apps@gmail.com">
        JLOJIC
    </author>

    <preference name="phonegap-version" value="2.9.0" /> 
    <preference name="orienation" value="default" />
    <feature name="http://api.phonegap.com/1.0/network"/>
    <access origin="http://www.jlojic.com" subdomains="true" />
    <icon src="icon.png" />


    <!-- Android -->
    <gap:platform name="android" />
    <preference name="android-minSdkVersion" value="8" />
    <preference name="android-targetSdkVersion" value="18" /> 
    
    <icon src="icons/android/icon36x36.png" gap:platform="android" gap:density="ldpi" />
    <icon src="icons/android/icon48x48.png" gap:platform="android" gap:density="mdpi" />
    <icon src="icons/android/icon72x72.png" gap:platform="android" gap:density="hdpi" />
    <icon src="icons/android/icon96x96.png" gap:platform="android" gap:density="xhdpi" />


    <gap:splash src="splash/android/splash200x320.png" gap:platform="android" gap:density="ldpi" width="200" height="320"/>
    <gap:splash src="splash/android/splash320x480.png" gap:platform="android" gap:density="mdpi" width="320" height="480"/>
    <gap:splash src="splash/android/splash480x800.png" gap:platform="android" gap:density="hdpi" width="480" height="800"/>
    <gap:splash src="splash/android/splash720x1280.jpg" gap:platform="android" gap:density="xhdpi" width="720" height="1280"/>


    <!-- iPhone and iPad -->



    <gap:platform name="ios"/>
	<!--
    <icon src="icons/ios/icon.png" gap:platform="ios" width="57" height="57" />
    <icon src="icons/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
    <icon src="icons/ios/icon_at_2x.png" gap:platform="ios" width="114" height="114" />


    <gap:platform name="symbian" /> -->



</widget>